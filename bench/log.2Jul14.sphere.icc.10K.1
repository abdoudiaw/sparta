SPARTA (5 Dec 2013)
# flow around a sphere on a uniform grid
# particles are created initially and input at boundaries at stream velocity

variable            x index 10
variable            y index 10
variable            z index 10

variable            n equal 10*$x*$y*$z
variable            n equal 10*10*$y*$z
variable            n equal 10*10*10*$z
variable            n equal 10*10*10*10
variable            fnum equal 7.33e+21/v_n

seed	    	    12345
dimension   	    3
global              nrho 7.03E18
global              fnum ${fnum}
global              fnum 7.33e+17
timestep 	    1.E-5
global              gridcut 0.1
global              surfmax 500

boundary	    o o o
create_box          -5.0 5.0 -5.0 5.0 -5.0 5.0
Created orthogonal box = (-5 -5 -5) to (5 5 5)
create_grid         $x $y $z
create_grid         10 $y $z
create_grid         10 10 $z
create_grid         10 10 10
Created 1000 child grid cells
  parent cells = 1
  CPU time = 0.000812054 secs
  create/ghost percent = 96.4181 3.58191

balance_grid        rcb cell
Balance grid migrated 0 cells
  CPU time = 0.00118613 secs
  reassign/sort/migrate/ghost percent = 9.78894 0.241206 1.76884 88.201

species		    ../data/ar.species Ar

mixture             air Ar frac 1.0
mixture 	    air group species
mixture             air Ar vstream 2500.0 0.0 0.0 temp 300.00

fix		    in inflow air all

collide		    vss air ../data/ar.vss

read_surf	    1 sdata.sphere
  602 points
  1200 triangles
  -1 1 xlo xhi
  -1 1 ylo yhi
  -1 1 zlo zhi
  0.100631 min triangle edge length
  0.00439601 min triangle area
  32 = cells with surfs
  1860 = total surfs in all grid cells
  216 = max surfs in one grid cell
  0.100631 = min surf-size/cell-size ratio
  968 0 32 = cells outside/inside/overlapping surfs
  32 = surf cells with 1,2,etc splits
  995.862 995.862 = cell-wise and global flow volume
  CPU time = 0.056006 secs
  read/surf2grid/error/ghost/inout percent = 1.2571 5.5635 91.9619 0.951444 0.266064
surf_collide	    1 diffuse 300.0 1.0
surf_modify         collide 1 1

create_particles    air n 0
Created 9551 particles
  CPU time = 0.00188017 secs

fix                 3 balance 100 1.1 rcb mol
collide_modify      vremax 100 yes

#compute             1 grid all ndensity mass u v w
#fix                 1 ave/grid 10 100 1000 c_1 ave running
#dump                1 grid 1000 tmp_flow.* id xc yc zc f_1

#compute             2 surf all press ke
#fix		    2 ave/surf 10 100 1000 c_2 ave running
#dump                2 surf 1000 tmp_surf.* id f_2

compute             g grid all n
compute             max reduce max c_g[1]

stats_style	    step cpu np nattempt ncoll nscoll c_max
stats		    100

# equilibrate flow for 1000 steps
# then benchmark for 1000 steps

run 		    1000
